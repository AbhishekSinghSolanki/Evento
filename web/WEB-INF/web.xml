<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Rlogin</servlet-name>
        <servlet-class>Rlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CorporateEvent</servlet-name>
        <servlet-class>CorporateEvent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LiveEvent</servlet-name>
        <servlet-class>LiveEvent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WeddingEvent</servlet-name>
        <servlet-class>WeddingEvent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Rcorpo</servlet-name>
        <servlet-class>Rcorpo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Rlive</servlet-name>
        <servlet-class>Rlive</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Rwed</servlet-name>
        <servlet-class>Rwed</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Rlogin</servlet-name>
        <url-pattern>/Rlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CorporateEvent</servlet-name>
        <url-pattern>/CorporateEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LiveEvent</servlet-name>
        <url-pattern>/LiveEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WeddingEvent</servlet-name>
        <url-pattern>/WeddingEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Rcorpo</servlet-name>
        <url-pattern>/Rcorpo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Rlive</servlet-name>
        <url-pattern>/Rlive</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Rwed</servlet-name>
        <url-pattern>/Rwed</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
